[
  {
    "type": "ACA.App",
    "name": "pocredis",

    "location": "eastus",
    "properties": {
      "environmentId": "${ENVIRONMENT}",
      "configuration": {
        "service": {
          "type": "redis"
        }
      }
    }
  },

  {
    "type": "ACA.App",
    "name": "poc",

    "location": "eastus",
    "properties": {
      "environmentId": "${ENVIRONMENT}",
      "template": {
        "containers": [
          {
            "image": "duglin/echo",
            "name": "echo"
          }
        ],
        "serviceBinds": [
          {
            "serviceId": "/subscriptions/${SUBSCRIPTION}/resourceGroups/${RESOURCEGROUP}/providers/Microsoft.App/containerapps/pocredis",
            "name": "pocredis"
          }
        ]
      }
    }
  }
]
